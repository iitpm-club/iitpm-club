{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"no-unused-vars": [
			"warn",
			{
				"args": "after-used",
				"argsIgnorePattern": "^_"
			}
		],
		"no-use-before-define": "error",
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"react/prop-types": "off"
	}
}
